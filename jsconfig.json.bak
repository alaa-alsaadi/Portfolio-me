{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "vueCompilerOptions": {
    "globalTypesPath": "./node_modules/vue/global.d.ts"
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}